<template>

</template>

<style>

</style>

<script>
import wepy from 'wepy'

export default class Index extends wepy.page {
    data = {
        nickName: ""
    }

    onLoad() {
        wx.getUserInfo({
            success: (data) => {
                let parsedData = JSON.parse(data.rawData)
                this.$parent.globalData.userInfo = parsedData
                this.nickName = parsedData.nickName
                this.$apply()
            }
        })
    }
}
</script>
